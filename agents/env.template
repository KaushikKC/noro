# LLM Provider API Keys (set at least one)
GEMINI_API_KEY=your_gemini_key_here        # recommended for Quick Start
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
DEEPSEEK_API_KEY=your_deepseek_key_here
OPENROUTER_API_KEY=your_openrouter_key_here

# Optional: Default LLM Settings
DEFAULT_LLM_PROVIDER=deepseek             # Using DeepSeek (paid account) - or openai / anthropic / gemini / openrouter
DEFAULT_MODEL=deepseek-chat               # DeepSeek model (or deepseek-reasoner for better reasoning)
GEMINI_MAX_TOKENS=20000                    # recommended context limit for Gemini
DEEPSEEK_MAX_TOKENS=8192                   # DeepSeek max limit (1-8192)

# Neo Blockchain Configuration
NEO_RPC_URL=http://localhost:20332         # Default Neo RPC endpoint
NEO_NETWORK=testnet                         # or mainnet
NEO_CONTRACT_HASH=0x76834b08fe30a94c0d7c722454b9a2e7b1d61e3a  # NoroMarket contract script hash

